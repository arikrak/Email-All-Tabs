{	
	"name": "Email All Tabs",
	"description": "This extension lets you email all tabs in current window", 
  
	"version": "0.31",
	"manifest_version": 2,

	  "background": {
    "scripts": ["script.js"]
  },
	
	"permissions": [
		"tabs"
	],
	
	"icons": {
		"16": "icon.png",
		"128": "icon_128.png"
	},	

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Email All Tabs"
	}
}