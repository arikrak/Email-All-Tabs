{	
	"name": "Email All Tabs",
	"description": "This extension lets you email all tabs in current window, and shortens long URLs", 
  
	"version": "0.60",
	"manifest_version": 2,

	"options_page": "options.html",

	  "background": {
    "scripts": ["script.js"]
  },
	
	"permissions": [
		"tabs",
		"storage",
		"https://www.googleapis.com/"

	],
	
	"icons": {
		"16": "icon.png",
		"128": "icon_128.png"
	},	

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Email All Tabs"
	}
}